{
	"name": "__MSG_extName__",
	"version": "2.3.3",
	"short_name": "Bookmarkie",
	"manifest_version": 3,
	"description": "__MSG_extDesc__",
	"icons": {
		"128": "icons/icon-128.png"
	},
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"default_icon": {
			"16": "icons/star-dark-16.png",
			"24": "icons/star-dark-24.png",
			"32": "icons/star-dark-32.png"
		},
		"default_popup": "popup.html",
		"default_title": "__MSG_extActionTitle__"
	},
	"options_ui": {
		"page": "options.html",
		"open_in_tab": false
	},
	"permissions": [
		"bookmarks",
		"tabs",
		"storage"
	],
	"minimum_chrome_version": "88",
	"default_locale": "en"
}
